    "contact_number": {
      "type": "string",
      "description": "Contact Number",
      "required": true,
      "regex": "^[0-9 ]*$",
      "minLength": 10,
      "maxLength": 15
    },
    "establishment_name": {
      "type": "string",
      "description": "Comapny Name",
      "required": true,
      "unique": true
    },
    "address_complement": {
      "type": "text"
    },
    "address_type": {
      "type": "string",
      "description": "Address Type"
    },
    "address_number": {
      "type": "string",
      "description": "Address Number",
      "required": true
    },
    "address_street": {
      "type": "string",
      "description": "Address Street",
      "required": true
    },
    "city": {
      "type": "string",
      "description": "City",
      "required": true
    },
    "country": {
      "default": "FRANCE",
      "type": "string",
      "description": "Country"
    },
    "postal_code": {
      "type": "string",
      "description": "Postal Code",
      "required": true,
      "minLength": 1,
      "maxLength": 5,
      "regex": "^[0-9]*$"
    },
    "h_orias": {
      "type": "string",
      "description": "RCS SIREN",
      "required": true
    },
    "e_siret": {
      "type": "string",
      "description": "SIRET",
      "required": true,
      "regex": "^[0-9A-Za-z]*$"
    },
    "h_company_rcs_siren": {
      "type": "string",
      "required": true,
      "regex": "^[0-9]*$"
    },
    "h_cif": {
      "default": false,
      "type": "boolean"
    },
    "h_coa": {
      "default": false,
      "type": "boolean"
    },
    "company_address": {
      "type": "text"
    },
    "reason": {
      "type": "text"
    },
    "website": {
      "type": "string"
    },
    "foundedYear": {
      "type": "string"
