    "linkedin": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "banner_image": {
      "type": "string"
    },
    "geolocation": {
      "type": "geography",
      "description": "Geolocation"
    },
    "youtube": {
      "type": "string",
      "description": "Youtube"
    },
    "instagram": {
      "type": "string",
      "description": "Instagram"
    },
    "logo": {
      "type": "string"
