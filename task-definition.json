{
    "executionRoleArn": "arn:aws:iam::392977643844:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/myapp",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "fargate"
                }
            },
            "portMappings": [
                {
                    "hostPort": 1337,
                    "protocol": "tcp",
                    "containerPort": 1337
                    
                }
            ],
            "image": "392977643844.dkr.ecr.us-west-2.amazonaws.com/cgp_strapi",
            "name": "cgp_strapi"
        }
    ],
    "memory": "512",
    "family": "strapi-task",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256"
   
}
